#include<iostream>
using namespace std; 
  
void swap(int *a, int *b)  
{  
    int temp = *a;  
    *a = *b;  
    *b = temp;  
}  
 
void ssort(int arr[], int n)  
{ 
    int i, j, min;  
    for (i = 0; i < n-1; i++)  
    {    
        min = i;  
        for (j = i+1; j < n; j++)  
        if (arr[j] < arr[min])  
            min= j;
        swap(&arr[min], &arr[i]);  
    }  
}  

  int main()  
{  
    int n,arr[100];
    cin>>n;
    for(int i=0;i<n;i++)
    cin>>arr[i]; 
    ssort(arr, n);  
    for (int i=0; i <n ; i++)  
        cout << arr[i] << " ";   
    return 0;  
}
